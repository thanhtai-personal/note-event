  "scripts": {
    "test": "mocha --exit test/*.test.js",
    "create-dev-tables": "node ./database/dbConnection createAllTables",
    "drop-dev-tables": "node ./database/dbConnection dropAllTables",
    "migrate-tables": "node ./database/dbConnection migrateData",
    "start": "nodemon --watch index",
    "debug": "nodemon --inspect index.js",
    "setup": "npm-run-all -p create-dev-tables start",
    "setupdev2": "npm-run-all -p create-dev2-tables start",
    "startWithConfig": "env-cmd -f ./.env nodemon src/app.js",
    "crawler": "node crawler/index.js"
  },
